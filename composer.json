{
	"name": "bluesnowman/leap",
	"description": "A data access layer and ORM for any PHP framework that is designed to work with all major SQL database dialects and drivers.",
	"version": "4.0.0",
	"type": "library",
	"homepage": "http://spadefoot.github.io/kohana-orm-leap/",
	"keywords": [
		"database",
		"sql",
		"orm",
		"mptt",
		"active record",
		"data mapper"
	],
	"license": "Apache-2.0",
	"require": {
		"php": ">=5.6.6"
	},
	"require-dev": {
		"php": ">=5.6.6"
	},
	"authors": [
		{
			"name": "Spadefoot Team",
			"email": "spadefoot.oss@gmail.com",
			"role": "developer"
		}
	],
	"support": {
		"issues": "https://github.com/bluesnowman/leap/issues",
		"source": "http://spadefoot.github.io/kohana-orm-leap/"
	},
	"suggest": {
		"ext-curl": "*",
		"ext-gd2": "*",
		"ext-interbase": "*",
		"ext-mssql": "*",
		"ext-mysql": "*",
		"ext-mysqli": "*",
		"ext-oci8": "*",
		"ext-pdo": "*",
		"ext-pgsql": "*"
	},
	"prefer-stable": true,
	"autoload": {
		"psr-0": {
			"Leap": "classes/"
		}
	}
}
